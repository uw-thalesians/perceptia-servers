{
        "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
        "contentVersion": "1.0.0.0",
        "parameters": {
            "location": {
                "type": "string"
            },
            "publicIpAddressName": {
                "type": "string"
            },
            "publicIpAddressNameDev": {
                "type": "string"
            },
            "publicIpAddressType": {
                "type": "string"
            },
            "publicIpAddressSku": {
                "type": "string"
            }
        },
        "resources": [
            {
                "name": "[parameters('publicIpAddressName')]",
                "type": "Microsoft.Network/publicIpAddresses",
                "apiVersion": "2018-08-01",
                "location": "[parameters('location')]",
                "properties": {
                    "publicIpAllocationMethod": "[parameters('publicIpAddressType')]"
                },
                "sku": {
                    "name": "[parameters('publicIpAddressSku')]"
                },
                "tags": {
                    "use": "capstone"
                }
            },
            {
                "name": "[parameters('publicIpAddressNameDev')]",
                "type": "Microsoft.Network/publicIpAddresses",
                "apiVersion": "2018-08-01",
                "location": "[parameters('location')]",
                "properties": {
                    "publicIpAllocationMethod": "[parameters('publicIpAddressType')]"
                },
                "sku": {
                    "name": "[parameters('publicIpAddressSku')]"
                },
                "tags": {
                    "use": "capstone"
                }
            }
        ],
        "outputs": {
            "publicIpAddressName": {
                "type": "string",
                "value": "[reference(resourceId('Microsoft.Network/publicIPAddresses',parameters('publicIpAddressName'))).IpAddress]"
            },
            "publicIpAddressNameDev": {
                "type": "string",
                "value": "[reference(resourceId('Microsoft.Network/publicIPAddresses',parameters('publicIpAddressNameDev'))).IpAddress]"
            }
        }
    }